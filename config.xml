<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kulturhuset.kulturhuset" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Kulturhuset</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <plugin name="cordova-plugin-transport-security" spec="~0.1.1" />
    <plugin name="cordova-plugin-whitelist" spec="~1.0.0" />
    <preference name="orientation" value="portrait" />
    <plugin name="cordova-plugin-device" spec="~1.1.1" />
    <plugin name="phonegap-plugin-push" spec="~1.5.3" />
</widget>
